# kernel/.cargo/config.toml for x86_64
[build]
target = "x86_64-unknown-none"

[unstable]
build-std = ["core", "compiler_builtins"]
build-std-features = ["compiler-builtins-mem"]

[target.x86_64-unknown-none]
rustflags = [
    "-C", "link-args=-Tlink.ld",
    "-C", "link-args=-e_start",
    "-C", "link-args=--oformat=elf64-x86-64",
    "-C", "link-args=-no-pie"
]